{
    "name": "php-core/binance-api",
    "description": "PHP Binance API is an asynchronous PHP library for the Binance API designed to be easy to use.",
    "keywords": ["Binance","cryptocurrency","WebSocket", "trading", "crypto"],
    "license": "MIT",
    "repositories": [ {
            "type": "vcs",
            "url": "https://github.com/php-core/binance-api"
    }],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ratchet/pawl": "^0.4.3 || ^0.4.0",
        "react/socket": "^1.16 || ^1.0 || ^0.8 || ^0.7",
        "ratchet/rfc6455": "^0.4 || ^0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "config": {
    	"optimize-autoloader": true,
        "minimum-stability": "stable"
    },
    "autoload": {
        "psr-4": {
            "PHPCore\\BinanceApi\\": "src/"
        }
    },
    "suggest": {
        "illuminate/support": "Required to use the Laravel service provider and facade (^8.0|^9.0|^10.0|^11.0|^12.0)."
    },
    "extra": {
        "laravel": {
            "providers": [
                "PHPCore\\BinanceApi\\Laravel\\BinanceApiServiceProvider"
            ],
            "aliases": {
                "Binance": "PHPCore\\BinanceApi\\Laravel\\Facades\\Binance"
            }
        }
    },
    "scripts": {
        "test": "phpunit tests/BinanceStaticTests.php",
        "test-coverage": "phpunit tests/BinanceStaticTests.php --coverage-html coverage/",
        "live-tests": "phpunit tests/BinanceLiveTests.php",
        "api-test": "phpunit tests/BinanceApiTest.php",
        "all-tests": "phpunit tests",
        "all-tests-coverage": "phpunit tests --coverage-html coverage/"
    }
}
